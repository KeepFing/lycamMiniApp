<!--pages/mine/mine.wxml-->
<view class='wa-list'>
<view class="weui-cells weui-cells_after-title" bindtap='userInfo'>
            <view class="weui-cell">
                <view class="weui-cell__hd">
                    <image src="{{headUrl}}" style="margin-right: 5px;vertical-align: middle;width:60px; height: 60px;border-radius: 60px"></image>
                </view>
                <view class="weui-cell__bd">{{username}}</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </view>
        </view>
<view class='min-space'></view>
</view>
        <view class="weui-cells weui-cells_after-title">
            <view class="weui-cell" bindtap='linkToMyOrder'>
                <view class="weui-cell__hd">
                    <image src="../../image/icon_user_indent.png" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
                </view>
                <view class="weui-cell__bd">我的订单</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </view>
            <view class="weui-cell" bindtap='linkToAboutUs'>
                <view class="weui-cell__hd">
                    <image src="../../image/iocn_user_about.png" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
                </view>
                <view class="weui-cell__bd">关于我们</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </view>
            <view class="weui-cell" bindtap='linkToFeedback'>
                <view class="weui-cell__hd">
                    <image src="../../image/icon_user_suggest.png" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
                </view>
                <view class="weui-cell__bd">意见反馈</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </view>
        </view>


